        "NOT_SUPPORT",
        "PNG",
        "GIF",
        "JPEG",
        "WEBP",
        "SVG",
        "HEIC"
      ],
      "default": "NOT_SUPPORT",
      "title": "圖片格式枚舉"
    },
    "mediaServiceImageMetadata": {
      "type": "object",
      "properties": {
        "width": {
          "type": "integer",
          "format": "int64",
          "title": "寬度必須大於0且小於10000"
        },
        "height": {
          "type": "integer",
          "format": "int64",
          "title": "高度必須大於0且小於10000"
        },
        "format": {
          "$ref": "#/definitions/mediaServiceImageFormat",
